{
  "name": "@technote-space/pr-commit-body-action",
  "version": "1.7.3",
  "description": "GitHub action to add commit history to PR body.",
  "keywords": [
    "github",
    "github actions"
  ],
  "homepage": "https://github.com/technote-space/pr-commit-body-action",
  "bugs": {
    "url": "https://github.com/technote-space/pr-commit-body-action/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/technote-space/pr-commit-body-action.git"
  },
  "license": "MIT",
  "author": {
    "name": "Technote",
    "email": "technote.space@gmail.com",
    "url": "https://technote.space"
  },
  "files": [
    "lib",
    "action.yml"
  ],
  "scripts": {
    "build": "rm -rdf lib && rollup -c",
    "cover": "vitest run --coverage",
    "postinstall": "[ -n \"$CI\" ] || [ ! -f node_modules/.bin/husky ] || husky install",
    "lint": "eslint 'src/**/*.ts' --cache",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "prepublishOnly": "[ -n \"$CI\" ] || [ ! -f node_modules/.bin/pinst ] || pinst --disable",
    "postpublish": "[ -n \"$CI\" ] || [ ! -f node_modules/.bin/pinst ] || pinst --enable",
    "release": "yarn release-ga --test",
    "test": "yarn lint && yarn typecheck && yarn cover",
    "typecheck": "tsc --noEmit",
    "update": "npm_config_yes=true npx npm-check-updates -u --timeout 100000 && yarn install && yarn upgrade && yarn audit"
  },
  "devDependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@octokit/openapi-types": "^17.1.1",
    "@octokit/plugin-paginate-rest": "^6.0.0",
    "@octokit/plugin-rest-endpoint-methods": "^7.0.1",
    "@octokit/types": "^9.2.1",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@sindresorhus/tsconfig": "^3.0.1",
    "@technote-space/filter-github-action": "^0.6.10",
    "@technote-space/github-action-helper": "^5.3.13",
    "@technote-space/github-action-log-helper": "^0.2.15",
    "@technote-space/github-action-test-helper": "^0.11.8",
    "@technote-space/github-action-version-helper": "^0.6.9",
    "@technote-space/release-github-actions-cli": "^1.9.5",
    "@types/node": "^20.1.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitest/coverage-c8": "^0.31.0",
    "eslint": "^8.40.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nock": "^13.3.1",
    "pinst": "^3.0.0",
    "rollup": "^3.21.5",
    "typescript": "^5.0.4",
    "update-section": "^0.3.3",
    "vitest": "^0.31.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
